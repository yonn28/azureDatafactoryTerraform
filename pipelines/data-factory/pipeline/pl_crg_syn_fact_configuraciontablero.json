				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": "SELECT CT.[GK_CONFIGURACION_TABLERO]\n      ,CT.[DESC_ANNIO]\n      ,CT.[DTM_FECHA_REGISTRO]\n      ,CT.[DESC_SIGLA_AREA]\n      ,CT.[FK_TABLERO]\n\t,CAST(REPLACE(Left(CAST(CT.[DTM_FECHA_REGISTRO] AS date),11),'-','') AS INT) AS FK_TIEMPO\n      ,CASE WHEN CONVR.[DESC_VERSION] IS NOT NULL\n\t          THEN CONVR.[DESC_VERSION]\n\t\t    ELSE '1.0' END AS [DESC_VERSION]\n      ,CT.[FK_EJE_PALANCA]\n      ,CT.[FK_OBJETIVO]\n      ,CT.[DESC_HITO_INDICADOR]\n      ,CT.[FK_PARAM_INDICADOR]\n      ,CT.[FK_INDICADOR]\n      ,CT.[DESC_TIPO_TABLERO]\n      ,CT.[DESC_TBG]\n      ,CT.[DESC_SECUENCIA_ORDEN]\n      ,CT.[VLR_PESO_INDICADOR]\n      ,CT.[DESC_CORTO_LARGO_PLAZO]\n      ,CT.[VLR_LIMITE_INFERIOR]\n      ,CT.[VLR_LIMITE_SUPERIOR]\n      ,CT.[DTM_FECHACARGA]\n      ,CT.[DESC_ORIGEN]\n      ,CT.[FK_TIPOSINCONSISTENCIA]\n      ,CT.[DESC_TABLERO_CVR]\n      ,CAST(CONCAT(CAST(GETDATE() AS DATE),' ',DATEPART(HOUR, dateadd(hour, -7, GETDATE())),':00:00.000')AS DATETIME) AS FECHA_CARGA_SYNAPSE\n      ,CAST(CONCAT(CAST(GETDATE() AS DATE),' ',DATEPART(HOUR, dateadd(hour, -5, GETDATE())),':00:00.000')AS DATETIME) AS FECHA_PROXIMA_ACTUALIZA_SYNAPSE\n      ,'' AS DESC_HITO_INDIC_HABILITADOR\n\t  ,IND2.[ACTIVOCUMPLIMIENTOMIN]\n      ,IND2.[CUMPLIMIENTOMINIMO]\n      ,IND2.[RECONOCIMIENTOMINIMO]\n\nFROM [ATOMO].[STG.DIM_CONFIGURACION_TABLERO] CT\n\tLEFT JOIN [ATOMO].[DWH.DIM_INDICADOR] IND\n\t\tON CT.[FK_INDICADOR] = IND.[GK_INDICADOR]\n\tLEFT JOIN [ATOMO].[DWH.DIM_PARAM_INDICADOR] DPI\n\t\tON CT.[FK_PARAM_INDICADOR] = DPI.[GK_PARAM_INDICADOR]\n\tLEFT JOIN [ATOMO].[STG.DIM_CVR_INDICADOR_HITO] CVRH\n\t\tON CT.[FK_INDICADOR] = CVRH.FKINDICADORHITO\n            AND DPI.DESC_DETALLE_INDICADOR = CVRH.DESCDETALLEINDICADORHITO\n\tLEFT JOIN [ATOMO].[STG.DIM_CONFIGURACION_CVR_UX] CONVR\n\t\tON CVRH.[FKCONFIGCVR] = CONVR.[GK_CVR]\n\tLEFT JOIN [ATOMO].[STG.FACT_CONFIGURACION_TABLERO_ILP] IND2\n\t\tON CT.[GK_CONFIGURACION_TABLERO] = IND2.[GK_CONFIGURACION_TABLERO]\nWHERE CT.DESC_SIGLA_AREA IS NOT NULL AND CT.DESC_SIGLA_AREA NOT LIKE '%ILP%'",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
